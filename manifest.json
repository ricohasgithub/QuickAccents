{
    "name": "QuickAccents",
    "version": "1.0",
    "description": "Acess your accents easily!",
    "manifest_version": 2,
    "browser_action": {
        "default_icon": "assets/Accent.png",
        "default_title": "Q&A",
        "default_popup": "src/html/popup.html"
    },
    "permissions": ["storage", "unlimitedStorage", "tabs", "<all_urls>"],
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "css": ["src/css/styles.css"]
    }]
}
